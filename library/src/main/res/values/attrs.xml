<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimePicker">
        <attr name="canvasColor" format="color" />
        <attr name="clockFaceColor" format="color" />
        <attr name="pointerColor" format="color" />
        <attr name="trackColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="disabledCanvasColor" format="color" />
        <attr name="disabledClockFaceColor" format="color" />
        <attr name="disabledPointerColor" format="color" />
        <attr name="disabledTrackColor" format="color" />
